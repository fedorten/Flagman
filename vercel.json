{
  "version": 2,
  "framework": null,
  "buildCommand": null,
  "installCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "routes": [
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "OPTIONS"]
    },
    {
      "src": "/static/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      },
      "dest": "index.html"
    }
  ]
}
